input_string = input("Enter the input:")
operands = input_string.split()
operand1 = int(operands[0])
operand2 = int(operands[2])
operator = operands[1]
if operator == "+":
    output = operand1 + operand2
elif operator == "-":
    output = operand1 - operand2
elif operator == "*":
    output = operand1 * operand2
elif operator == "/":
    output = operand1 / operand2
elif operator == "%":
    output = operand1 % operand2
else:
    print("provide the proper input")
print (output)
